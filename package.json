{
  "name": "gameboxd-web",
  "version": "1.0.0",
  "description": "Gameboxd - Gaming Social Platform (Web Version)",
  "main": "server.js",
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "react-scripts build",
    "dev": "concurrently \"npm start\" \"cd ../Gameboxd/vgj-backend && node --watch server.js\"",
    "install-all": "npm install && cd ../vgj-backend && npm install"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "lucide-react": "^0.574.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000"
}
